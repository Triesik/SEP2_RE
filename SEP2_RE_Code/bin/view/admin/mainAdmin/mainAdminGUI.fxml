<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" style="-fx-background-color: darkgray;" xmlns="http://javafx.com/javafx/8.0.202-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.admin.mainAdmin.mainAdminGUI">
   <children>
      <TabPane layoutX="21.0" layoutY="13.0" prefHeight="734.0" prefWidth="758.0" style="-fx-background-color: white;" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="tabMainView" text="Main View">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="677.0" prefWidth="758.0" />
            </content>
          </Tab>
          <Tab fx:id="tabAddUser" text="Add User">
              <fx:include fx:id="embeddedAddUser" source="../addUser/addUserGUI.fxml" />
          </Tab>
            <Tab fx:id="tabEditUser" text="Edit User">
                <fx:include fx:id="embeddedEditUser" source="../editUser/editUserGUI.fxml" />
            </Tab>
            <Tab fx:id="tabRemoveUser" text="Remove User">
                <fx:include fx:id="embeddedRemoveUser" source="../removeUser/removeUserGUI.fxml" />
            </Tab>
        </tabs>
      </TabPane>
      <Button fx:id="btnLogout" layoutX="716.0" layoutY="760.0" mnemonicParsing="false" onAction="#logoutButtonPressed" text="Logout" />
   </children>
</AnchorPane>
